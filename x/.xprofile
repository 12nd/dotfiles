#1/bin/sh

# Runs on DM startup if using one

xrdb -merge $HOME/.Xresources

# no beep
xset -b

# Autostart stuffs
compton -b -f & # compositor
setwp & # wallpaper
sxhkd & # hotkey daemon
remapkb & # remap keyboard caps esc thing
dunst & # notifs
unclutter & # hides mouse after delay
redshift -l 37.8136:144.9631 & # f.lux style with melbourne
#mpd & # music
#mpdscribble & # music last.fm
urxvtd -f & # terminal
ibus-daemon -drx & # CJK IME

