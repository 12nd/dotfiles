[alias]
	c = commit
	p = push
	s = status
	su = status --untracked-files=normal
	sua = status -u
	rv = remote -v
	a = add
	aa = add .
	co = checkout
	cam = commit -a -m
	cm = commit -m
	d = diff
	cob = checkout -b
	l = log
	tree = log --graph --oneline --all
